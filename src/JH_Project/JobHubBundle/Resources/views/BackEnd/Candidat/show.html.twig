{% extends "JobHubBundle::admin_layout.html.twig" %}

{% block page_title %}
<h2>Candidat</h2>
{% endblock %}

{% block page_content %}
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Username</th>
                <td>{{ entity.compteUser.username }}</td>
            </tr>
            <tr>
                <th>Usernamecanonical</th>
                <td>{{ entity.compteUser.usernameCanonical }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.compteUser.email }}</td>
            </tr>
            <tr>
                <th>Emailcanonical</th>
                <td>{{ entity.compteUser.emailCanonical }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ entity.compteUser.enabled }}</td>
            </tr>
            <tr>
                <th>Salt</th>
                <td>{{ entity.compteUser.salt }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ entity.compteUser.password }}</td>
            </tr>
            <tr>
                <th>Lastlogin</th>
                <td>{{ entity.compteUser.lastLogin|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Locked</th>
                <td>{{ entity.compteUser.locked }}</td>
            </tr>
            <tr>
                <th>Expired</th>
                <td>{{ entity.compteUser.expired }}</td>
            </tr>
            <tr>
                <th>Confirmationtoken</th>
                <td>{{ entity.compteUser.confirmationToken }}</td>
            </tr>
            <tr>
                <th>Passwordrequestedat</th>
                <td>{{ entity.compteUser.passwordRequestedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ entity.compteUser.roles }}</td>
            </tr>
            <tr>
                <th>Credentialsexpired</th>
                <td>{{ entity.compteUser.credentialsExpired }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ entity.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ entity.adresse }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ entity.telephone }}</td>
            </tr>
            <tr>
                <th>Siteperso</th>
                <td>{{ entity.sitePerso }}</td>
            </tr>
            <tr>
                <th>Niveauetudes</th>
                <td>{{ entity.niveauEtudes }}</td>
            </tr>
            <tr>
                <th>Codepostal</th>
                <td>{{ entity.codePostal }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('administration_candidat') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('administration_candidat_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
